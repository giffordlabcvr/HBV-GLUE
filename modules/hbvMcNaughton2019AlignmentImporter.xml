<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<blastFastaAlignmentImporter>
  <idClauseExtractorFormatter>
    <matchPattern><![CDATA[(?<seqID>(?:NC_)?[A-Za-z0-9]+)$]]></matchPattern>
    <outputTemplate>source.name in ('ncbi-refseqs', 'ncbi-outgroups') and sequenceID = '${seqID}'</outputTemplate>
  </idClauseExtractorFormatter>
    <minRowCoveragePercent>90.0</minRowCoveragePercent>
</blastFastaAlignmentImporter>
