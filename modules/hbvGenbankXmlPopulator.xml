<genbankXmlPopulator>
	<gbPrimaryAccession>
		<propertyPopulator property="gb_primary_accession"/>
	</gbPrimaryAccession>
	<gbAccessionVersion>
		<propertyPopulator property="gb_accession_version"/>
	</gbAccessionVersion>
	<gbLength>
		<propertyPopulator property="length"/>
	</gbLength>
	<gbUpdateDate>
		<propertyPopulator property="gb_update_date"/>
	</gbUpdateDate>
	<gbCreateDate>
		<propertyPopulator property="gb_create_date"/>
	</gbCreateDate>
	<gbReference>
		<gbRefPubmed>
			<propertyPopulator property="pubmed_id"/>
		</gbRefPubmed>
	</gbReference>
	<gbDivision>
		<propertyPopulator property="lab_construct">
			<matchPattern>PAT</matchPattern>
			<outputTemplate>true</outputTemplate>
		</propertyPopulator>
	</gbDivision>
	<gbFeature key="source">
		<gbQualifier name="isolate">
			<propertyPopulator property="isolate"/>
		</gbQualifier>

		<gbQualifier name="country">
			<isoCountryPropertyPopulator property="country_iso" codeStyle="ALPHA_3"/>
			<isoCountryPropertyPopulator property="country_official" codeStyle="OFFICIAL"/>
			<propertyPopulator property="country"/>
		</gbQualifier>
		
		
		<gbQualifier name="host">
			<propertyPopulator property="host_species">

				<!-- #### Primates -->					
				<valueConverter>
					<matchPattern>.*[Hh]omo [Ss]apiens.*</matchPattern>
					<matchPattern>.*[Hh]uman.*</matchPattern>
					<outputTemplate>Homo sapiens</outputTemplate>
				</valueConverter>
				<valueConverter>
					<matchPattern>.*[Jj]apanese monkey.*</matchPattern>
					<outputTemplate>Macaca fuscata</outputTemplate>
				</valueConverter>

			</propertyPopulator>
		</gbQualifier>
		<gbQualifier name="collection_date">
			<propertyPopulator property="collection_month_day">
				<matchPattern>(\d{2})-[A-Za-z]{3}-\d{4}</matchPattern>
				<outputTemplate>${g1}</outputTemplate>
			</propertyPopulator>
			<propertyPopulator property="collection_month">
				<matchPattern>\d{2}-([A-Za-z]{3})-\d{4}</matchPattern>
				<outputTemplate>${g1?upper_case}</outputTemplate>
			</propertyPopulator>
			<propertyPopulator property="collection_year">
				<matchPattern>(\d{4})</matchPattern>
				<outputTemplate>${g1}</outputTemplate>
			</propertyPopulator>
		</gbQualifier>
		
		
	</gbFeature>
</genbankXmlPopulator>