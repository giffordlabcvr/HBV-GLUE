<figTreeAnnotationExporter>
	<figTreeAnnotation>
		<annotationName>host</annotationName>
		<valueFreemarkerTemplate>${renderNestedProperty('sequence.host_species')}</valueFreemarkerTemplate>
	</figTreeAnnotation>
	<figTreeAnnotation>
		<annotationName>major_clade</annotationName>
		<valueFreemarkerTemplate>${renderNestedProperty('sequence.genotype')}</valueFreemarkerTemplate>
	</figTreeAnnotation>
	<figTreeAnnotation>
		<annotationName>minor_clade</annotationName>
		<valueFreemarkerTemplate>${renderNestedProperty('sequence.subgenotype')}</valueFreemarkerTemplate>
	</figTreeAnnotation>
	<figTreeAnnotation>
		<annotationName>sequenceID</annotationName>
		<valueFreemarkerTemplate>${renderNestedProperty('sequence.sequenceID')}</valueFreemarkerTemplate>
	</figTreeAnnotation>
	<figTreeAnnotation>
		<annotationName>combined</annotationName>
		<valueFreemarkerTemplate>${renderNestedProperty('sequence.sequenceID')}/${renderNestedProperty('sequence.genotype')}/${renderNestedProperty('sequence.subgenotype')}/${renderNestedProperty('sequence.host_species')}}</valueFreemarkerTemplate>
	</figTreeAnnotation>
</figTreeAnnotationExporter>