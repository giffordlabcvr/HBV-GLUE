<maxLikelihoodGenotyper>
	<maxLikelihoodPlacerModuleName>rabvMaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>
	<cladeCategory>
		<name>major_clade</name>	
		<displayName>Major clade</displayName>	
		<whereClause>clade_category = 'major_clade'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
		<distanceCutoff>0.4</distanceCutoff>
		<finalCladeCutoff>75.0</finalCladeCutoff>
	</cladeCategory>
	<cladeCategory>
		<name>minor_clade</name>	
		<displayName>Minor clade</displayName>	
		<whereClause>clade_category = 'minor_clade'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
	    <distanceCutoff>0.26</distanceCutoff>
		<finalCladeCutoff>61.0</finalCladeCutoff>
	</cladeCategory>
</maxLikelihoodGenotyper>